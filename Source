-- Speed
local BGS = Window:NewTab("Speed")
local SecondSection = BGS:NewSection("Speed")

SecondSection:NewButton("Load Script", "loads the Speed script", function()
    local plr = game.Players.LocalPlayer
	local char = plr.Character

	char.Humanoid.WalkSpeed =500000
end)
